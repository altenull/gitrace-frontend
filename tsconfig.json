{
  "compilerOptions": {
    "target": "ES5",
    "module": "ESnext",
    "strict": true,
    "esModuleInterop": true,
    "outDir": "dist",
    "lib": ["DOM", "DOM.iterable", "ESNext"],
    "allowJs": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve"
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx"],
  "exclude": ["node_modules"]
}
